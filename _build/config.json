{
  "name": "VloX",
  "lowCaseName": "vlox",
  "description": "Simple integration to easily use VueJs inside modx",
  "author": "@camicase82",
  "version": "0.0.1",
  "package": {
    "actions": [{
      "id": 1,
      "controller": "init",
      "hasLayout": 1,
      "langTopics": "package:default",
      "assets": ""
    }],
    "menus": [
      {
        "text": "Trotalo",
        "description": "ModX with super powers",
        "action": "",
        "parent": "topnav",
        "icon": "",
        "menuIndex": 4,
        "params": "",
        "handler": ""
      },
      {
      "text": "VloX",
      "description": "Your Vue components",
      "action": "init",
      "parent": "Trotalo",
      "icon": "",
      "menuIndex": 0,
      "params": "",
      "handler": ""
    }],
    "elements": {
      "plugins": [
        {
          "name": "vuexBlocksTab",
          "file": "vuexBlocksTab.php",
          "events": [
            "OnDocFormPrerender"
          ]
        }
      ],
      "snippets": [
        {
          "name": "generateVueComponentsFiles",
          "file": "generateVueComponentsFiles.php"
        },
        {
          "name": "vloxRenderer",
          "file": "vloxRenderer.php"
        },
        {
          "name": "vlox",
          "file": "vlox.php"
        },
        {
          "name": "vueImportRenderer",
          "file": "vueImportRenderer.php"
        },
        {
          "name": "generateSchema",
          "file": "generateSchema.php"
        },
        {
          "name": "updateAndRun",
          "file": "updateAndRun.php"
        }
      ],
      "chunks": [
        {
          "name": "head",
          "file": "head.html"
        },
        {
          "name": "defaultApp",
          "file": "defaultApp.html"
        },
        {
          "name": "mainJs",
          "file": "main.js"
        }
      ],
      "templates": [
        {
          "name": "vloxTemplate",
          "file": "vloxTemplate.html"
        }
      ]
    }
  },
  "database": {
    "tables": [
      "vloxResourceContent",
      "vloxBlocks"
    ]
  },
  "build":{
    "readme": "docs/readme.txt",
    "license": "docs/license.txt",
    "changelog": "docs/changelog.txt"
  }
}